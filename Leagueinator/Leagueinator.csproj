<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\default.style" />
    <None Remove="Assets\default.xml" />
    <None Remove="Assets\EventScoreForm.style" />
    <None Remove="Assets\EventScoreForm.xml" />
    <None Remove="Assets\ScoreRow.xml" />
    <None Remove="Assets\TeamScore.xml" />
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Include="Assets\default.style" />
    <EmbeddedResource Include="Assets\default.xml" />
    <EmbeddedResource Include="Assets\EventScoreForm.style" />
    <EmbeddedResource Include="Assets\EventScoreForm.xml" />
    <EmbeddedResource Include="Assets\ScoreRow.xml" />
    <EmbeddedResource Include="Assets\TeamScore.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.Drawing.Common" Version="8.0.4" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Model\Model.csproj" />
    <ProjectReference Include="..\Printer\Printer.csproj" />
  </ItemGroup>

</Project>
